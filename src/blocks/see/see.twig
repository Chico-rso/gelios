<section class="see">
	<div class="container">
		<h2>Вы смотрели</h2>
		<div class="see__wrapper _see__wrapper">
			{% for item in ["./img/see-1.png", "./img/see-2.png", "./img/see-3.png", "./img/see-4.png", "./img/see-5.png","./img/see-6.png"] %}
				{{ item }}
			<div class="see__card">
				<div class="see__image">
					<div class="see__statuses">
						<div class="see__status see__status--sale">Экономия 20%</div>
						<div class="see__status see__status--none">Нет в наличии</div>
						<div class="see__status see__status--new">Новинка</div>
						<div class="see__status see__status--balance">2 штуки</div>
					</div>
					<a href="#">
						<picture>
							<source srcset="./img/see-1.png" media="(min-width: 769px)">
								<img src="{{ item }}" alt="product image">
								{% include "../../assets/svg/popular-goods-favorit.svg" %}
							</picture>
						</a>
					</div>
					<div class="see__title">
						<a href="#">Вимакс Супер Форте капс. № 30</a>
					</div>
					<div class="see__raits" data-rating="2.5">
						<div class="see__rait-star" >
							<i></i>
							<i></i>
							<i></i>
							<i></i>
							<i></i>
							<span>12</span>
						</div>
						<div class="see__rait-notstar">
							<span>Нет отзывов</span>
						</div>
					</div>
					<div class="see__price">
						<span class="see__price-sale">258.20 ₽</span>
						<span class="see__price-none">335,66 ₽</span>
						<span class="see__price-default">335 ₽</span>
					</div>
				</div>
				{% endfor %}
			</div>
		</div>
	</section>
